#!/usr/bin/env octave -q
starting=5;
bse=10;
t=8;
n=input("");
if(n==1)
	t=11;
endif
while(t>0)
	n=n+starting-2;
	digits=1;
	expbase=1;
	x=bse-1;
	while(x<=n)
		n=n-x;
		digits=digits+1;
		expbase=expbase*bse;
		x=digits*expbase*(bse-1);
	endwhile
	num=expbase+idivide(n,digits,"floor");
	d=digits-1-mod(n,digits);
	while(d>0)
		num=idivide(num,bse,"floor");
		d=d-1;
	endwhile
	printf("%d\n",mod(num,bse));
	t=t-1;
	if(t>0)
		n=input("");
	endif
endwhile